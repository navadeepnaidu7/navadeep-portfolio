{
  "version": 1,
  "include": ["/*"],
  "exclude": [
    "/assets/*",
    "/*.js",
    "/*.css",
    "/*.svg",
    "/*.png",
    "/*.jpg",
    "/*.jpeg",
    "/*.webp",
    "/*.ico",
    "/*.woff",
    "/*.woff2"
  ],
  "rules": [
    {
      "type": "rewrite",
      "include": ["/*"],
      "exclude": ["/assets/*"],
      "destination": "/index.html"
    }
  ]
}
